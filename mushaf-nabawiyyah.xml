<quran>
    <!-- Muka surat 1 / Mula surah al-fatihah -->
    <surah indeks="1" nama="الفَاتِحَةِ" namatranslit="Al-Fātiḥah" juz="1" jumlat="7" kronologi="5" origin="Makkiyyah" hal="1">
        <ayat indeks="1">بِسۡمِ ٱللَّهِ ٱلرَّحۡمَٰنِ ٱلرَّحِيمِ</ayat>
        <ayat indeks="2">ٱلۡحَمۡدُ لِلَّهِ رَبِّ ٱلۡعَٰلَمِينَ</ayat>
        <ayat indeks="3">ٱلرَّحۡمَٰنِ ٱلرَّحِيمِ</ayat>
        <ayat indeks="4">مَٰلِكِ يَوۡمِ ٱلدِّينِ</ayat>
        <ayat indeks="5">إِيَّاكَ نَعۡبُدُ وَإِيَّاكَ نَسۡتَعِينُ</ayat>
        <ayat indeks="6">ٱهۡدِنَا ٱلصِّرَٰطَ ٱلۡمُسۡتَقِيمَ</ayat>
        <ayat indeks="7">صِرَٰطَ ٱلَّذِينَ أَنۡعَمۡتَ عَلَيۡهِمۡ غَيۡرِ ٱلۡمَغۡضُوبِ عَلَيۡهِمۡ وَلَا ٱلضَّآلِّينَ</ayat>
    </surah>

    <!-- Muka surat 2 / Mula surah al-baqarah -->
    <surah indeks="2" nama="البَقَرَةِ" namatranslit="Al-Baqarah" juz="1-3" jumlat="286" kronologi="87" origin="Madaniyyah, kecuali ayat 281 turun di Mina ketika Haji yang terakhir." hal="2-49">
        <ayat indeks="1">الٓمٓ</ayat>
        <ayat indeks="2">ذَٰلِكَ ٱلۡكِتَٰبُ لَا رَيۡبَۛ فِيهِۛ هُدٗى لِّلۡمُتَّقِينَ</ayat>
        <ayat indeks="3">ٱلَّذِينَ يُؤۡمِنُونَ بِٱلۡغَيۡبِ وَيُقِيمُونَ ٱلصَّلَوٰةَ وَمِمَّا رَزَقۡنَٰهُمۡ يُنفِقُونَ</ayat>
        <ayat indeks="4">وَٱلَّذِينَ يُؤۡمِنُونَ بِمَآ أُنزِلَ إِلَيۡكَ وَمَآ أُنزِلَ مِن قَبۡلِكَ وَبِٱلۡأٓخِرَةِ هُمۡ يُوقِنُونَ</ayat>
        <ayat indeks="5">أُوْلَٰٓئِكَ عَلَىٰ هُدٗى مِّن رَّبِّهِمۡۖ وَأُوْلَٰٓئِكَ هُمُ ٱلۡمُفۡلِحُونَ</ayat>
        <ayat indeks="6">إِنَّ ٱلَّذِينَ كَفَرُواْ سَوَآءٌ عَلَيۡهِمۡ ءَأَنذَرۡتَهُمۡ أَمۡ لَمۡ تُنذِرۡهُمۡ لَا يُؤۡمِنُونَ</ayat>
    </surah>

    <!-- Muka surat 50 / Mula surah ali imran -->
    <surah indeks="3" nama="آلِ عِمۡرَانَ" namatranslit="Āli ‘Imrān" juz="3-4" jumlat="200" kronologi="89" origin="Madaniyyah" hal="50-76">
        <ayat indeks="1"></ayat>
        <ayat indeks="2"></ayat>
        <ayat indeks="3"></ayat>
        <ayat indeks="4"></ayat>
        <ayat indeks="5"></ayat>
        <ayat indeks="6"></ayat>
        <ayat indeks="7">هُوَ ٱلَّذِيٓ أَنزَلَ عَلَيۡكَ ٱلۡكِتَٰبَ مِنۡهُ ءَايَٰتٞ مُّحۡكَمَٰتٌ هُنَّ أُمُّ ٱلۡكِتَٰبِ وَأُخَرُ مُتَشَٰبِهَٰتٞۖ فَأَمَّا ٱلَّذِينَ فِي قُلُوبِهِمۡ زَيۡغٞ فَيَتَّبِعُونَ مَا تَشَٰبَهَ مِنۡهُ ٱبۡتِغَآءَ ٱلۡفِتۡنَةِ وَٱبۡتِغَآءَ تَأۡوِيلِهِۦۖ وَمَا يَعۡلَمُ تَأۡوِيلَهُۥٓ إِلَّا ٱللَّهُۗ وَٱلرَّٰسِخُونَ فِي ٱلۡعِلۡمِ يَقُولُونَ ءَامَنَّا بِهِۦ كُلّٞ مِّنۡ عِندِ رَبِّنَاۗ وَمَا يَذَّكَّرُ إِلَّآ أُوْلُواْ ٱلۡأَلۡبَٰبِ</ayat>
    </surah>

